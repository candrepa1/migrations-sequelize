<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Balance</th>
      <th scope="col">Date Opened</th>
      <th scope="col">Account Type ID</th>
      <th scope="col">Customer ID</th>
    </tr>
  </thead>
  <tbody>
      {{#each accounts}}
        <tr>
            <th scope="row">{{this.id}}</th>
            <td>{{this.balance}}</td>
            <td>{{this.date_opened}}</td>
            <td>{{this.account_types_id}}</td>
            <td>{{this.customers_id}}</td>
        </tr>
      {{/each}}
  </tbody>
</table>